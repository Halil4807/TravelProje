@using TravelProje.Models.Siniflar
@model List<Blog>
<div class="padding">
    <h4>Son 10 Seyahatlerim</h4>
    <ul class="list list-pad">
        @{int count = 0;}
        @foreach (var item in Model)
        {
            count++;
            <li class="list-bg"><span>@count.</span><a href="/Blog/BlogDetay/@item.ID">@item.Baslik</a></li>
        }
    </ul>
    <a href="#" class="link1">See all hotels</a>
</div>